<div id="post-container"></div>
    <div>
        <h2>Your Dashboard</h2>
    </div>

    <div id="editPost-card">
        <div>
            <h3>Edit Post</h3>
        </div>
        <form id="editPost-form">
            <div>
                <label for="editPost-title">Title</label>
                <input type="input" id="newPost-title" />
            </div>
            <div>
                <label for="editPost-content">Content</label>
                <input type="input" id="editPost-content" />
            </div>
            <div>
                <button id="editPost-submit" type="submit">Update Post</button>
            </div>
            <div>
                <button id="editPost-delete" type="submit">Delete</button>
            </div>
        </form>
    </div>

    <div>
        <button class="add-newPost" type="button">+ New Post</button>
    </div>

</div>

<script src="/js/post.js"></script>